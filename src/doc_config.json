{
  "model_name": "codellama:13b",
  "temperature": 0.3,
  "max_tokens": 1500,
  "code_extensions": [
    ".cs",
    ".js",
    ".kt",
    ".py",
    ".swift",
    ".scala",
    ".cpp",
    ".tsx",
    ".rs",
    ".php",
    ".h",
    ".go",
    ".c",
    ".jsx",
    ".ts",
    ".rb",
    ".java"
  ],
  "ignore_dirs": [
    "venv",
    "__pycache__",
    ".env",
    ".pytest_cache",
    ".git",
    "build",
    ".next",
    "obj",
    "node_modules",
    "bin",
    ".nuxt",
    "dist",
    "env",
    "target"
  ],
  "ignore_files": [
    "*.min.js",
    ".env*",
    "package-lock.json",
    "yarn.lock",
    "*.min.css",
    ".gitignore"
  ],
  "max_file_size": 1048576,
  "batch_size": 10,
  "max_files_to_document": 10,
  "min_lines_for_detailed_doc": 20,
  "output_filename": "PROJECT_DOCUMENTATION.md",
  "include_technical_appendix": true,
  "include_complexity_analysis": true
}